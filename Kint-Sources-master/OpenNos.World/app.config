<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="eventScheduler" type="OpenNos.GameObject.EventSchedule, OpenNos.GameObject" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <add name="OpenNosContext" providerName="System.Data.SqlClient" connectionString="Data Source=localhost;Initial Catalog=opennos;Integrated Security=true" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <log4net>
    <appender name="ManagedColoredConsoleAppender" type="log4net.Appender.ManagedColoredConsoleAppender">
      <mapping>
        <level value="FATAL" />
        <foreColor value="DarkRed" />
      </mapping>
      <mapping>
        <level value="ERROR" />
        <foreColor value="Red" />
      </mapping>
      <mapping>
        <level value="WARN" />
        <foreColor value="DarkYellow" />
      </mapping>
      <mapping>
        <level value="INFO" />
        <foreColor value="white" />
      </mapping>
      <mapping>
        <level value="DEBUG" />
        <foreColor value="Blue" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%date{HH:mm:ss}][%level]%message%newline" />
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log.xml" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="20" />
      <maximumFileSize value="1GB" />
      <layout type="log4net.Layout.XmlLayout" />
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="ManagedColoredConsoleAppender" />
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
  <appSettings>
	<add key="MasterAuthKey" value="OpenNosTest" />
	<add key="MasterPort" value="6969" />
	<add key="MasterIP" value="127.0.0.1" />
	<add key="ChatLogKey" value="ChatLogKey" />
	<add key="ChatLogPort" value="42069" />
	<add key="Language" value="en" />
	<add key="WorldPort" value="3825" />
	<add key="IPAddress" value="173.249.28.51" />
	<add key="ServerGroup" value="S1-NosHeat" />
	<add key="UseMock" value="false" />
	<add key="LagMode" value="false" />
	<add key="Act4Port" value="5100" />
	<add key="MaxBankGold" value="100000000000" />
	<add key="RateReput" value="2" />
  </appSettings>
  <eventScheduler>
    <schedule event="LOD" time="02:00" />
    <schedule event="LOD" time="07:00" />
    <schedule event="LOD" time="12:00" />
    <schedule event="LOD" time="16:00" />
    <schedule event="LOD" time="22:00" />
    <schedule event="CALIGOR" time="19:00" />
    <schedule event="TALENTARENA" time="08:00" />
    <schedule event="METEORITEGAME" time="12:00" />
    <schedule event="METEORITEGAME" time="16:00" />
    <schedule event="METEORITEGAME" time="20:00" />
    <schedule event="METEORITEGAME" time="00:00" />
    <schedule event="RANKINGREFRESH" time="03:00" />
    <schedule event="MINILANDREFRESHEVENT" time="00:00" />
    <schedule event="ICEBREAKER" time="00:00" />
    <schedule event="ICEBREAKER" time="01:00" />
    <schedule event="ICEBREAKER" time="03:00" />
    <schedule event="ICEBREAKER" time="05:00" />
    <schedule event="ICEBREAKER" time="07:00" />
    <schedule event="ICEBREAKER" time="09:00" />
    <schedule event="ICEBREAKER" time="11:00" />
    <schedule event="ICEBREAKER" time="13:00" />
    <schedule event="ICEBREAKER" time="15:00" />
    <schedule event="ICEBREAKER" time="17:00" />
    <schedule event="ICEBREAKER" time="19:00" />
    <schedule event="ICEBREAKER" time="21:00" />
    <schedule event="ICEBREAKER" time="23:00" />
    <schedule event="INSTANTBATTLE" time="01:55" />
    <schedule event="INSTANTBATTLE" time="03:55" />
    <schedule event="INSTANTBATTLE" time="05:55" />
    <schedule event="INSTANTBATTLE" time="07:55" />
    <schedule event="INSTANTBATTLE" time="09:55" />
    <schedule event="INSTANTBATTLE" time="11:55" />
    <schedule event="INSTANTBATTLE" time="13:55" />
    <schedule event="INSTANTBATTLE" time="15:55" />
    <schedule event="INSTANTBATTLE" time="17:55" />
    <schedule event="INSTANTBATTLE" time="19:55" />
    <schedule event="INSTANTBATTLE" time="21:55" />
    <schedule event="INSTANTBATTLE" time="23:55" />
  </eventScheduler>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>
  <runtime>
    <gcServer enabled="true" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.ValueTuple" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>